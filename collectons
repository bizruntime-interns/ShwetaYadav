	}
    class  HashCollection
    {
    static void Main()
    {
      Hashtable ht=new Hashtable();
      ht.Add("id",1010);
      ht.Add("name",shweta);
      ht.Add("age",21);
      Console.WriteLine(ht["name "]);

     foreach (object key in ht.Keys)
       Console.WriteLine(key);
     Console.ReadLine();
    }
    }
   }
